{
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\": "database/"
    }
  },
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "nikic/fast-route": "^1.3",
    "nesbot/carbon": "^3.10",
    "zircote/swagger-php": "^5.5"
  },
  "require-dev": {
    "pestphp/pest": "^4.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": [
      "./vendor/bin/pest"
    ],
    "swagger": [
      "./vendor/bin/openapi app -o swagger.json"
    ]
  }
}
